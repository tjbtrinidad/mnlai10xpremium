{
  "name": "mnl-ai-premium-website",
  "version": "2.0.0",
  "description": "Premium AI-powered website and automation platform for Filipino entrepreneurs. Built by Tristan Trinidad.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Tests not implemented yet\" && exit 0",
    "lint": "eslint . --ext .js",
    "format": "prettier --write .",
    "build": "echo \"No build process needed for vanilla setup\" && exit 0",
    "deploy": "echo \"Ready for Railway deployment\" && npm start"
  },
  "keywords": [
    "ai",
    "websites",
    "automation",
    "filipino",
    "entrepreneurs",
    "web-design",
    "chatbots",
    "premium",
    "modern",
    "responsive"
  ],
  "author": {
    "name": "Tristan Trinidad",
    "email": "hello@mnl-ai.com",
    "url": "https://mnl-ai.com"
  },
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mnl-ai/premium-website.git"
  },
  "bugs": {
    "url": "https://github.com/mnl-ai/premium-website/issues",
    "email": "hello@mnl-ai.com"
  },
  "homepage": "https://mnl-ai.com",
  "config": {
    "port": 3000
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 100
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true,
      "browser": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn"
    }
  }
}